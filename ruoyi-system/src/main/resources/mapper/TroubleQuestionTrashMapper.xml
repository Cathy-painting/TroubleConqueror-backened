<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.TroubleQuestionTrashMapper">
    
    <resultMap type="TroubleQuestionTrash" id="TroubleQuestionTrashResult">
        <result property="trashId"    column="trash_id"    />
        <result property="questionId"    column="question_id"    />
        <result property="userId"    column="user_id"    />
        <result property="questionContent"    column="question_content"    />
        <result property="questionImages"    column="question_images"    />
        <result property="answerContent"    column="answer_content"    />
        <result property="answerImages"    column="answer_images"    />
        <result property="questionType"    column="question_type"    />
        <result property="tags"    column="tags"    />
        <result property="deleteReason"    column="delete_reason"    />
        <result property="deleteBy"    column="delete_by"    />
        <result property="deleteTime"    column="delete_time"    />
        <result property="originalCreateTime"    column="original_create_time"    />
        <result property="originalCreateBy"    column="original_create_by"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectTroubleQuestionTrashVo">
        select trash_id, question_id, user_id, question_content, question_images, answer_content, answer_images, question_type, tags, delete_reason, delete_by, delete_time, original_create_time, original_create_by, remark from trouble_question_trash
    </sql>

    <select id="selectTroubleQuestionTrashList" parameterType="TroubleQuestionTrash" resultMap="TroubleQuestionTrashResult">
        <include refid="selectTroubleQuestionTrashVo"/>
        <where>  
            <if test="questionId != null "> and question_id = #{questionId}</if>
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="questionContent != null  and questionContent != ''"> and question_content like concat('%', #{questionContent}, '%')</if>
            <if test="questionImages != null  and questionImages != ''"> and question_images = #{questionImages}</if>
            <if test="answerContent != null  and answerContent != ''"> and answer_content like concat('%', #{answerContent}, '%')</if>
            <if test="answerImages != null  and answerImages != ''"> and answer_images = #{answerImages}</if>
            <if test="questionType != null  and questionType != ''"> and question_type = #{questionType}</if>
            <if test="tags != null  and tags != ''"> and tags like concat('%', #{tags}, '%')</if>
            <if test="deleteReason != null  and deleteReason != ''"> and delete_reason like concat('%', #{deleteReason}, '%')</if>
            <if test="deleteBy != null  and deleteBy != ''"> and delete_by = #{deleteBy}</if>
            <if test="deleteTime != null  and deleteTime != ''"> and delete_time = #{deleteTime}</if>
            <if test="originalCreateTime != null  and originalCreateTime != ''"> and original_create_time = #{originalCreateTime}</if>
            <if test="originalCreateBy != null  and originalCreateBy != ''"> and original_create_by = #{originalCreateBy}</if>
        </where>
        order by delete_time desc
    </select>
    
    <select id="selectTroubleQuestionTrashByTrashId" parameterType="Long" resultMap="TroubleQuestionTrashResult">
        <include refid="selectTroubleQuestionTrashVo"/>
        where trash_id = #{trashId}
    </select>

    <select id="selectTroubleQuestionTrashByQuestionId" parameterType="Long" resultMap="TroubleQuestionTrashResult">
        <include refid="selectTroubleQuestionTrashVo"/>
        where question_id = #{questionId}
    </select>
        
    <insert id="insertTroubleQuestionTrash" parameterType="TroubleQuestionTrash" useGeneratedKeys="true" keyProperty="trashId">
        insert into trouble_question_trash
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="questionId != null">question_id,</if>
            <if test="userId != null">user_id,</if>
            <if test="questionContent != null and questionContent != ''">question_content,</if>
            <if test="questionImages != null">question_images,</if>
            <if test="answerContent != null">answer_content,</if>
            <if test="answerImages != null">answer_images,</if>
            <if test="questionType != null">question_type,</if>
            <if test="tags != null">tags,</if>
            <if test="deleteReason != null">delete_reason,</if>
            <if test="deleteBy != null">delete_by,</if>
            <if test="deleteTime != null">delete_time,</if>
            <if test="originalCreateTime != null">original_create_time,</if>
            <if test="originalCreateBy != null">original_create_by,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="questionId != null">#{questionId},</if>
            <if test="userId != null">#{userId},</if>
            <if test="questionContent != null and questionContent != ''">#{questionContent},</if>
            <if test="questionImages != null">#{questionImages},</if>
            <if test="answerContent != null">#{answerContent},</if>
            <if test="answerImages != null">#{answerImages},</if>
            <if test="questionType != null">#{questionType},</if>
            <if test="tags != null">#{tags},</if>
            <if test="deleteReason != null">#{deleteReason},</if>
            <if test="deleteBy != null">#{deleteBy},</if>
            <if test="deleteTime != null">#{deleteTime},</if>
            <if test="originalCreateTime != null">#{originalCreateTime},</if>
            <if test="originalCreateBy != null">#{originalCreateBy},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateTroubleQuestionTrash" parameterType="TroubleQuestionTrash">
        update trouble_question_trash
        <trim prefix="SET" suffixOverrides=",">
            <if test="questionId != null">question_id = #{questionId},</if>
            <if test="userId != null">user_id = #{userId},</if>
            <if test="questionContent != null">question_content = #{questionContent},</if>
            <if test="questionImages != null">question_images = #{questionImages},</if>
            <if test="answerContent != null">answer_content = #{answerContent},</if>
            <if test="answerImages != null">answer_images = #{answerImages},</if>
            <if test="questionType != null">question_type = #{questionType},</if>
            <if test="tags != null">tags = #{tags},</if>
            <if test="deleteReason != null">delete_reason = #{deleteReason},</if>
            <if test="deleteBy != null">delete_by = #{deleteBy},</if>
            <if test="deleteTime != null">delete_time = #{deleteTime},</if>
            <if test="originalCreateTime != null">original_create_time = #{originalCreateTime},</if>
            <if test="originalCreateBy != null">original_create_by = #{originalCreateBy},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where trash_id = #{trashId}
    </update>

    <delete id="deleteTroubleQuestionTrashByTrashId" parameterType="Long">
        delete from trouble_question_trash where trash_id = #{trashId}
    </delete>

    <delete id="deleteTroubleQuestionTrashByTrashIds" parameterType="String">
        delete from trouble_question_trash where trash_id in 
        <foreach item="trashId" collection="array" open="(" separator="," close=")">
            #{trashId}
        </foreach>
    </delete>
</mapper>


